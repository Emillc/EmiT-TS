{
  "name": "emit-ts",
  "type": "module",
  "version": "0.0.0",
  "description": "_description_",
  "author": {
    "name": "ChillCicada",
    "email": "2210227279@qq.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/emillc/emit-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emillc/emit-ts.git"
  },
  "bugs": "https://github.com/emillc/emit-ts/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "bun src/index.ts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "postinstall": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.13.0",
    "@antfu/ni": "0.23.2",
    "@antfu/utils": "0.7.10",
    "@types/node": "22.10.5",
    "bumpp": "9.10.0",
    "changelogen": "0.5.7",
    "eslint": "9.17.0",
    "eslint-plugin-format": "0.1.3",
    "lint-staged": "15.3.0",
    "rimraf": "6.0.1",
    "simple-git-hooks": "2.11.1",
    "typescript": "5.7.3",
    "unbuild": "2.0.0",
    "vite": "5.4.11",
    "vitest": "3.0.4"
  },
  "simple-git-hooks": {
    "pre-commit": "bun x lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ]
}
